{
  "name": "react-redux-webpack",
  "version": "0.1.0",
  "description": "React Redux Webpack: Starter Kit",
  "keywords": [
    "React",
    "JSX",
    "Webpack",
    "React Router",
    "ESLint"
  ],
  "homepage": "https://ifundeasy.github.io/react-redux-webpack",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "analyze:build": "npm run compile:theme && cross-env BUILD_GOAL=production NODE_ENV=production USE_ANALYZE=1 webpack --config ./webpack/webpack.prod.js --stats-error-details --profile --json=compilation-stats.json",
    "build:dev": "npm run compile:theme && cross-env BUILD_GOAL=dev NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "build:production": "npm run compile:theme && cross-env BUILD_GOAL=production NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "build:test": "npm run compile:theme && cross-env BUILD_GOAL=test NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "compile:theme": "npx gulp less",
    "deploy:github": "gh-pages -d dist",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "eslint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "fakeapi": "nodemon fakeapi/index.js",
    "lint": "npm run eslint && npm run stylelint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "markdownlint": "markdownlint *.md",
    "markdownlint:fix": "markdownlint --fix *.md",
    "predeploy:github": "npm run build:production",
    "preinstall": "npx pm-keeper npm",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix": "prettier --write \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix:all": "prettier --write . --end-of-line auto --ignore-unknown",
    "push:tags": "git push --follow-tags origin pre-gh-pages",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "start": "npm run compile:theme && cross-env BUILD_GOAL=development NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --stats-error-details",
    "start:dev": "concurrently -r \"npm run start\" \"npm run fakeapi\"",
    "stylelint": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "stylelint:fix": "stylelint \"src/**/*.less\" --fix --custom-syntax postcss-less",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:jest": "jest",
    "update:npm": "ncu -u && npm install"
  },
  "author": "wkylin",
  "contributors": [
    {
      "name": "ifundeasy",
      "email": "ifundeasy@gmail.com",
      "url": "https://github.com/ifundeasy"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifundeasy/react-redux-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/ifundeasy/react-redux-webpack/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
